        String words3 = words[2];
        String words4 = words[3];
        String words5 = words[4];
        String words6 = words[5];
        String words7 = words[6];
        String words8 = words[7];
        String words9 = words[8];
        String words10 = words[9];
        String words11 = words[10];

        // Виведення речення у консоль
